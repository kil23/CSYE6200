package edu.neu.csye6200;

import java.util.ArrayList;
import java.util.List;

public class ZooAnimalAPI {
	
	public static void demo() {
		AnimalAPI animal = new AnimalAPI();
		Cat cat = new Cat();
		Dog dog = new Dog();
		Fish fish = new Fish();
		
		List<AnimalAPI> list = new ArrayList<>();
		list.add(animal);
		list.add(cat);
		list.add(dog);
		list.add(fish);
		
		for(AnimalAPI animalAPI : list) {
			animalAPI.speak();
		}
	}
}
