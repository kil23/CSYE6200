package edu.neu.csye6200;

import java.util.ArrayList;
import java.util.List;

public class ZooAbstractAnimalAPI {
	
	public static void demo() {
		AnimalAPI animal = new AnimalAPI();
		Cat1 cat = new Cat1();
		Dog1 dog = new Dog1();
		Fish1 fish = new Fish1();
		
		List<AnimalAPI> list = new ArrayList<>();
		list.add(animal);
		list.add(cat);
		list.add(dog);
		list.add(fish);
		
		for(AnimalAPI animalAPI : list) {
			animalAPI.speak();
		}
	}
}
