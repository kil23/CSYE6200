package edu.neu.csye6200;

import java.util.ArrayList;
import java.util.List;

public class ZooAnimalisticAPI {
	
	public static void demo() {
		AnimalisticCat cat = new AnimalisticCat();
		AnimalisticDog dog = new AnimalisticDog();
		Fish3 fish = new Fish3();
		
		List<AnimalisticAPI> list = new ArrayList<>();
		list.add(cat);
		list.add(dog);
		list.add(fish);
		
		for(AnimalisticAPI animalisticAPI : list) {
			animalisticAPI.speak();
		}
	}
}
