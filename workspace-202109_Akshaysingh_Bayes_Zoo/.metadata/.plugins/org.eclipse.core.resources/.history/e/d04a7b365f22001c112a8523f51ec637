package edu.neu.csye6200;

import java.util.ArrayList;
import java.util.List;

public class ZooAnimalisticAPI {
	
	public static void demo() {
		Cat2 cat = new Cat2();
		Dog2 dog = new Dog2();
		Fish2 fish = new Fish2();
		
		List<AnimalisticAPI> list = new ArrayList<>();
		list.add(cat);
		list.add(dog);
		list.add(fish);
		
		for(AnimalAPI animalAPI : list) {
			animalAPI.speak();
		}
	}
}
